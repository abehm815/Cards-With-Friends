        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AndroidExample"
        tools:targetApi="31">
        <service android:name=".services.MusicService" />

        <!-- New Account Activity -->
